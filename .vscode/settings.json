{
    "cSpell.enabled": true,
    "cSpell.language": "en,pt,pt_BR",
    "cSpell.diagnosticLevel": "Warning",
    "cSpell.enableFiletypes": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "json",
        "markdown"
    ],
    "cSpell.customDictionaries": {
        "project-words": {
            "name": "Project Dictionary",
            "path": "${workspaceRoot}/cspell.json",
            "description": "Words used in this project",
            "addWords": true
        }
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.cspell": true
    },
    "cSpell.words": [
        "codauxiliar",
        "codprod",
        "descricao",
        "diasrestantes",
        "gestao",
        "mobileprovision",
        "Op√ßoes",
        "Pressable",
        "Reseta",
        "Swipeable"
    ]
}