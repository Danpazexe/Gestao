{
  "expo": {
    "name": "gestao",
    "slug": "Gestao",
    "version": "0.0.26",
    "orientation": "portrait",
    "icon": "./assets/Image/LOGO1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Image/LOGO1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.avante.Gestao",
      "icon": "./assets/Image/LOGO1024.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Image/LOGO432.png",
        "backgroundColor": "#ffffff",
        "monochromeImage": "./assets/Image/LOGO432.png"
      },
      "icon": "./assets/Image/LOGO432.png",
      "package": "com.avante.gestao",
      "permissions": [
        "CAMERA",
        "INTERNET",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM",
        "POST_NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/Image/LOGO1024.png"
    },
    "extra": {
      "eas": {
        "projectId": "8e4744f6-9b2a-497f-99bd-f44b4a10cfb9"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD"
    },
    "privacy": "public",
    "githubUrl": "https://github.com/usuario/gestao",
    "plugins": [
      "expo-local-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/Image/LOGO432.png",
          "color": "#ffffff",
          "sounds": ["./assets/Sound/notification.wav"],
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/Image/LOGO432.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Nova notificação"
    }
  }
}
