{
  "expo": {
    "name": "gestao",
    "slug": "Gestao",
    "version": "0.0.7",
    "orientation": "portrait",
    "icon": "./assets/LogoApp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LogoAppSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.avante.Gestao"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoAppAdapt.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.avante.gestao",
      "permissions": [
        "CAMERA",
        "INTERNET"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/LogoApp.png"
    },
    "extra": {
      "eas": {
        "projectId": "8e4744f6-9b2a-497f-99bd-f44b4a10cfb9"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "privacy": "public",
    "githubUrl": "https://github.com/usuario/gestao"
  }
}
